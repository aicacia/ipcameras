{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"productName": "ipcameras",
	"version": "0.1.0",
	"identifier": "com.ipcameras",
	"build": {
		"beforeBuildCommand": "pnpm build",
		"beforeDevCommand": "pnpm web",
		"devUrl": "http://localhost:5173",
		"frontendDist": "../build"
	},
	"app": {
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "IPCameras",
				"width": 800
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"category": "Utility",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"targets": "all"
	}
}
